{{#if photosAuthed}}
    {{#each photo}}
        <a href="{{url}}" data-sub-html="<h4>{{title}}</h4>">
            <div class="admin_edit_controls photo_controls">
                <span class="edit" id="{{_id}}"><i class="fas fa-pencil-alt"></i></span>
                <span class="delete" id="{{_id}}" data-collection="photos">[X]</span>
            </div>
            <img class="photos" src="{{url}}" alt="No image preview"/>
            <div class="photo_overlay"><i class="fa fa-plus-circle" aria-hidden="true"></i></div>
        </a>
    {{/each}}
{{else}}
    {{#each photo}}
        <a href="{{url}}" data-sub-html="<h4>{{title}}</h4>">
            <img class="photos" src="{{url}}" alt="No image preview"/>
            <div class="photo_overlay"><i class="fa fa-plus-circle" aria-hidden="true"></i></div>
        </a>
    {{/each}}
{{/if}}


